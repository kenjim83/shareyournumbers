<h1>All Revenue Posts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Movie</th>
      <th>Distributor</th>
      <th>Revenue</th>
      <th>Published</th>
      <th>Source url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @revenue_posts.each do |revenue_post| %>
      <tr>
        <td><%= revenue_post.user %></td>
        <td><%= revenue_post.movie %></td>
        <td><%= revenue_post.distributor %></td>
        <td><%= revenue_post.revenue %></td>
        <td><%= revenue_post.published %></td>
        <td><%= revenue_post.source_url %></td>
        <td><%= link_to 'Show', revenue_post %></td>
        <td><%= link_to 'Edit', edit_revenue_post_path(revenue_post) %></td>
        <td><%= link_to 'Destroy', revenue_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create New Revenue Post', new_revenue_post_path, class: "button radius" %>
